sort.month <- function(x, forest_fires = NULL, abbreviated = FALSE){
y <- forest_fires(m1 = month.name, m2 = month.abb, n = 1:12)
z <- if(abbreviated) match(x, y[, 'm2']) else match(x, y[, 'm1'])
x <- if(is.null(forest_fires)) x else forest_fires
h <- forest_fires(z, x)
h[order(z), ][, -1]
}
